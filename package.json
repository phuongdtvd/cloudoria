{
  "name": "phaserquest",
  "version": "1.0.0",
  "private": true,
  "description": "Clone of Mozilla's BrowserQuest with the Phaser engine",
  "main": "server.js",
  "scripts": {
    "dev": "run-p start serve",
    "start:db": "sudo mongod",
    "start": "node server.js",
    "serve": "vite",
    "docker:build:run": "bash scripts/docker-build-and-run.sh",
    "docker:start": "docker-compose up -d",
    "docker:restart:map": "bash scripts/docker-restart-map.sh",
    "docker:build": "docker-compose build",
    "docker:rebuild": "bash scripts/docker-rebuild.sh",
    "map:format": "node -e 'require(\"./js/server/format\").format()'",
    "build": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jerenaux/phaserquest"
  },
  "keywords": [
    "Browserquest",
    "Phaser",
    "online",
    "game",
    "html5"
  ],
  "author": "Jerome Renaux",
  "license": "MIT",
  "homepage": "https://github.com/Jerenaux/phaserquest#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.441.0",
    "clone": "^2.1.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongodb": "^6.2.0",
    "optimist": "^0.6.1",
    "pathfinding": "^0.4.18",
    "phaser-ce": "^2.20.0",
    "quickselect": "^2.0.0",
    "random-weighted-choice": "^0.1.4",
    "resolve-cwd": "^3.0.0",
    "socket.io": "^2.5.0"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "vite": "4.5.0"
  },
  "pnpm": {
    "overrides": {
      "minimist@<0.2.1": ">=0.2.1",
      "minimist@<0.2.4": ">=0.2.4"
    }
  }
}
